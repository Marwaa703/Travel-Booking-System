{
  "expo": {
    "name": "voyage",
    "slug": "voyage",
    "scheme": "voyage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#005C78"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "VIBRATE"
      ],
      "notification": {
        "icon": "./assets/adaptive-icon.png",
        "color": "#000000"
      }
    },
    "extra": {
      "eas": {
        "projectId": "31d7ba31-2f46-49ad-aa8c-990784280e2a"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
